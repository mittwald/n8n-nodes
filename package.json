{
	"name": "n8n-nodes-mittwald",
	"version": "0.0.0",
	"description": "n8n nodes for interacting with the mittwald API",
	"license": "MIT",
	"packageManager": "pnpm@10.28.2",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "Daniel Haering",
		"email": "d.haering@mittwald.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mittwald/n8n-nodes.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"release": {
		"branches": [
			{
				"name": "master"
			},
			{
				"name": "beta",
				"channel": "beta",
				"prerelease": true
			}
		]
	},
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/MittwaldApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Mittwald/Mittwald.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/npm": "^13.1.3",
		"@semantic-release/release-notes-generator": "^14.1.0",
		"eslint": "9.32.0",
		"n8n-workflow": "^2.6.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2",
		"vitest": "^4.0.18",
		"zod": "^3.25.67"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
